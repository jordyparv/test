test('should set filterObj for remedy_switch', () => {
    const req = { remedy_switch: 'someValue' };
    const res = { status: jest.fn().mockReturnThis(), json: jest.fn() };

    yourFunction(req, res);

    expect(filterObj.key).toBe('remedy_switch');
    expect(filterObj.value).toBe('expectedValue');
});

test('should set filterObj for remedy_callout', () => {
    const req = { remedy_callout: 'anotherValue' };
    const res = { status: jest.fn().mockReturnThis(), json: jest.fn() };

    yourFunction(req, res);

    expect(filterObj.key).toBe('remedy_callout');
    expect(filterObj.value).toBe('expectedValue');
});
