CREATE TABLE tmpkws (
    kwName VARCHAR2(255),
    kwDefaultValue VARCHAR2(255),
    kwDescription VARCHAR2(255),
    kwDataType VARCHAR2(50),
    kwRequired NUMBER(1),
    kwCategory VARCHAR2(255),
    kwOffset NUMBER,
    kwValue_String VARCHAR2(255),
    kwValue_Number NUMBER,
    kwValue_DateTime TIMESTAMP(6),
    kwAlias VARCHAR2(255),
    kwValue VARCHAR2(255),
    kwDisplayValue VARCHAR2(255),
    META_UNIVERSAL_ID VARCHAR2(128) UNIQUE,
    META_CREATED_DATE TIMESTAMP(6),
    META_CREATED_BY VARCHAR2(255),
    META_LAST_UPDATE_DATE TIMESTAMP(6),
    META_LAST_UPDATE_BY VARCHAR2(255),
    VALUE_META_UNIVERSAL_ID VARCHAR2(36),
    VALUE_META_CREATED_DATE TIMESTAMP(6),
    VALUE_META_CREATED_BY VARCHAR2(255),
    VALUE_META_LAST_UPDATE_DATE TIMESTAMP(6),
    VALUE_META_LAST_UPDATE_BY VARCHAR2(255)
);
