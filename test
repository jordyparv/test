 const controller = new AbortController();
        const timeoutId = setTimeout(() => controller.abort(), 10000); // Set timeout to 10 seconds

        const response = await fetch('https://nominatim.openstreetmap.org/search?q=la&format=jsonv2', {
            signal: controller.signal,
            headers: {
                'User-Agent': 'YourAppName/1.0', // Change this to your app name
            }
        });

        clearTimeout(timeoutId); // Clear timeout

        if (!response.ok) {
            throw new Error(`HTTP error! status: ${response.status}`);
        }

        const data = await response.json();
        console.log(data);
    } catch (error) {
        console.error('Fetch error:', error);
    }
