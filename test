SELECT TRIGGER_NAME, TRIGGER_TYPE, TRIGGERING_EVENT, STATUS, DESCRIPTION
FROM USER_TRIGGERS
WHERE TRIGGER_NAME = 'SWITCH_DETAILS_AUDIT_LOG';

--
ALTER TRIGGER IOPADMIN.SWITCH_DETAILS_AUDIT_LOG COMPILE;
--
SHOW ERRORS TRIGGER IOPADMIN.SWITCH_DETAILS_AUDIT_LOG;

--
SELECT OBJECT_NAME, OBJECT_TYPE
FROM ALL_DEPENDENCIES
WHERE NAME = 'SWITCH_DETAILS_AUDIT_LOG';
