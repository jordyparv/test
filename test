loadSwitchService.prototype.insertDataIntoDB = function (
  query,
  data,
  callback
) {
  if (!query) {
    callback("Please Provide the query to perform on db");
  }
  if (data.length === 0) {
     callback("Please Provide the data to insert in db");
  }
  let errors = [];
  let sqlQuery = queryFactory.getQuery(query);
  data.forEach((_data) => {
    oraUtil.execute(sqlQuery, _data, (err, results) => {
      if (err) {
        errors.push(err);
      }
    });
  });

  if (errors.length !== 0) {
  callback(errors);
  }
else
   callback();
};
