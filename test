loadSwitchService.prototype.getDummyData = async function (key) {
  
  return {
    switchElement: require("../dummy/SWITCH_ELEMENTS.json"),
    switchInstallElement: require("../dummy/SWITCH_INSTALLED_ELEMENTS.json"),
    switchInstallSubElement: require("../dummy/SWITCH_INSTALLED_SUBELEMENTS.json"),
  }[key];
};


loadSwitchService.prototype.populate_switch_installed_elements = function (
  callback
) {
  async.waterfall(
    [
      // async.constant(
      //   queryFactory.getQuery("getSwitchInstalledElementsFromOPS")
      // ),
      // loadSwitchService.prototype.getDataFromOpsTracker,
      async.constant("switchInstallElement"),
      loadSwitchService.prototype.getDummyData,
      loadSwitchService.prototype.parseSwitchInstalledElements,
      // loadSwitchService.prototype.insertDataIntoDB,
    ],
    function (err) {
      if (err) {
        logger.error(err);
        return callback(err);
      } else {
        logger.info("switch installed element data Populated Successfully!");
      }
      callback();
    }
  );
};

loadSwitchService.prototype.parseSwitchInstalledElements = function (
  data,
  callback,
) {
  console.log(data[0])
  if (data.length > 0) {
   return  callback("no data available for switch installed elements");
  }
  let switchInstalledElementData = [];
  data.forEach((item) => {
    let switchInstalledElementTMP = {
      LOCATION_ID: item["LocationId"],
      ELEMENT_ID: item["ElementId"],
      START_DATE: item["StartDate"],
      END_DATE: item["EndDate"],
      META_UNIVERSAL_ID: item["META_UNIVERSALID"],
      META_CREATED_DATE:
        moment(item["META_CREATEDDATE"]).format("YYYY-MM-DD HH:mm:ss") || null,
      META_CREATED_BY: item["META_CREATEDBY"],
      META_LAST_UPDATED_DATE:
        moment(item["META_LASTUPDATEDATE"]).format("YYYY-MM-DD HH:mm:ss") ||
        null,
      META_LAST_UPDATED_BY: item["META_LASTUPDATEBY"],
      NOTIFY_OVERRIDE: item["NOTIFY_OVERRIDE"],
    };
    switchInstalledElementData.push(switchInstalledElementTMP);
  });

  return callback(null,"syncSwitchInstalledElements", switchInstalledElementData);
};


