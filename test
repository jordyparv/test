function generateUpdateQuery(tableName, updateValues, keyColumns) {
    // Construct the SET clause
    const setClauses = Object.keys(updateValues).map(key => {
        return `${key} = :${key}`;
    }).join(', ');

    // Construct the WHERE clause
    const whereClauses = keyColumns.map(key => {
        return `${key} = :${key}`;
    }).join(' AND ');

    // Combine all parts into the final SQL query
    const sql = `UPDATE ${tableName} SET ${setClauses} WHERE ${whereClauses};`;

    return sql;
}
