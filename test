CREATE TABLE "IOPADMIN"."SWITCH_INSTALLED_ELEMENTS"
  (
  "LOCATION_ID" NUMBER NOT NULL ENABLE,
  "ELEMENT_ID" NUMBER NOT NULL ENABLE,
  "START_DATE" TIMESTAMP (6) NOT NULL ENABLE,
  "END_DATE" TIMESTAMP (6) NOT NULL ENABLE,
  "META_UNIVERSAL_ID" CHAR(32 BYTE) NOT NULL ENABLE,
  "META_CREATED_DATE" TIMESTAMP (6) NOT NULL ENABLE,
  "META_CREATED_BY" VARCHAR2(225 BYTE) NOT NULL ENABLE,
  "META_LAST_UPDATED_DATE" TIMESTAMP (6) NOT NULL ENABLE,
  "META_LAST_UPDATED_BY" VARCHAR2(225 BYTE) NOT NULL ENABLE,
  "NOTIFY_OVERRIDE" VARCHAR2(225 BYTE) NOT NULL ENABLE,
CONSTRAINT "PK_SWITCH_INSTALLED_ELEMENTS" PRIMARY KEY ("LOCATION_ID", "ELEMENT_ID", "START_DATE"));
CREATE UNIQUE INDEX "IOPADMIN"."IX_SWITCH_INSTALLED_ELEMENTS_META_UNIVERSAL_ID" ON "IOPADMIN"."SWITCH_INSTALLED_ELEMENTS" ("META_UNIVERSAL_ID");
