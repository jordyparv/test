MERGE INTO SWITCH_INSTALLED_ELEMENTS tgt
USING (SELECT :META_UNIVERSAL_ID AS id FROM dual) src
ON (src.id = tgt.META_UNIVERSAL_ID)
WHEN MATCHED THEN
    UPDATE SET
        tgt.LOCATION_ID = :LOCATION_ID,
        tgt.ELEMENT_ID = :ELEMENT_ID,
        tgt.START_DATE = TO_TIMESTAMP(:START_DATE, 'YYYY-MM-DD"T"HH24:MI:SS.FF'),
        tgt.END_DATE = TO_TIMESTAMP(:END_DATE, 'YYYY-MM-DD"T"HH24:MI:SS.FF'),
        tgt.META_CREATED_DATE = TO_TIMESTAMP(:META_CREATED_DATE, 'YYYY-MM-DD HH24:MI:SS'),
        tgt.META_CREATED_BY = :META_CREATED_BY,
        tgt.META_LAST_UPDATED_DATE = TO_TIMESTAMP(:META_LAST_UPDATED_DATE, 'YYYY-MM-DD HH24:MI:SS'),
        tgt.META_LAST_UPDATED_BY = :META_LAST_UPDATED_BY,
        tgt.NOTIFY_OVERRIDE = :NOTIFY_OVERRIDE
WHEN NOT MATCHED THEN
    INSERT (
        LOCATION_ID,
        ELEMENT_ID,
        START_DATE,
        END_DATE,
        META_UNIVERSAL_ID,
        META_CREATED_DATE,
        META_CREATED_BY,
        META_LAST_UPDATED_DATE,
        META_LAST_UPDATED_BY,
        NOTIFY_OVERRIDE
    )
    VALUES (
        :LOCATION_ID,
        :ELEMENT_ID,
        TO_TIMESTAMP(:START_DATE, 'YYYY-MM-DD"T"HH24:MI:SS.FF'),
        TO_TIMESTAMP(:END_DATE, 'YYYY-MM-DD"T"HH24:MI:SS.FF'),
        :META_UNIVERSAL_ID,
        TO_TIMESTAMP(:META_CREATED_DATE, 'YYYY-MM-DD HH24:MI:SS'),
        :META_CREATED_BY,
        TO_TIMESTAMP(:META_LAST_UPDATED_DATE, 'YYYY-MM-DD HH24:MI:SS'),
        :META_LAST_UPDATED_BY,
        :NOTIFY_OVERRIDE
    );
