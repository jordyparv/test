DECLARE
  -- Define the bind variables
  v_ELEMENT_ID NUMBER := 202;
  v_DESCRIPTION VARCHAR2(255) := '';
  v_ELEMENT_DIVISION VARCHAR2(255) := 'MTCE';
  v_ELEMENT_DEPT VARCHAR2(255) := 'VRAN';
  v_ELEMENT_SUB_DEPT VARCHAR2(255) := 'SS/VDU';
  v_IS_DISABLED NUMBER := 0;
  v_META_UNIVERSAL_ID VARCHAR2(255) := '03933C5FF1BAC1236DACACB07BB60E78';
  v_META_CREATED_DATE DATE := TO_DATE('2023-07-19 09:48:01', 'YYYY-MM-DD HH24:MI:SS');
  v_META_CREATED_BY VARCHAR2(255) := 'c2';
  v_META_LAST_UPDATED_DATE DATE := TO_DATE('2023-07-19 09:48:01', 'YYYY-MM-DD HH24:MI:SS');
  v_META_LAST_UPDATED_BY VARCHAR2(255) := 'c2';
  v_NO_DIRTRACKER NUMBER := 0;
BEGIN
  MERGE INTO SWITCH_ELEMENTS tgt
  USING (SELECT :v_META_UNIVERSAL_ID AS id FROM dual) src
  ON (src.id = tgt.META_UNIVERSAL_ID)
  WHEN MATCHED THEN
    UPDATE SET
      tgt.DESCRIPTION = :v_DESCRIPTION,
      tgt.ELEMENT_DIVISION = :v_ELEMENT_DIVISION,
      tgt.ELEMENT_DEPT = :v_ELEMENT_DEPT,
      tgt.ELEMENT_SUB_DEPT = :v_ELEMENT_SUB_DEPT,
      tgt.IS_DISABLED = :v_IS_DISABLED,
      tgt.META_CREATED_DATE = :v_META_CREATED_DATE,
      tgt.META_CREATED_BY = :v_META_CREATED_BY,
      tgt.META_LAST_UPDATED_DATE = :v_META_LAST_UPDATED_DATE,
      tgt.META_LAST_UPDATED_BY = :v_META_LAST_UPDATED_BY,
      tgt.NO_DIRTRACKER = :v_NO_DIRTRACKER
  WHEN NOT MATCHED THEN
    INSERT (
      ELEMENT_ID,
      DESCRIPTION,
      ELEMENT_DIVISION,
      ELEMENT_DEPT,
      ELEMENT_SUB_DEPT,
      IS_DISABLED,
      META_UNIVERSAL_ID,
      META_CREATED_DATE,
      META_CREATED_BY,
      META_LAST_UPDATED_DATE,
      META_LAST_UPDATED_BY,
      NO_DIRTRACKER
    )
    VALUES (
      :v_ELEMENT_ID,
      :v_DESCRIPTION,
      :v_ELEMENT_DIVISION,
      :v_ELEMENT_DEPT,
      :v_ELEMENT_SUB_DEPT,
      :v_IS_DISABLED,
      :v_META_UNIVERSAL_ID,
      :v_META_CREATED_DATE,
      :v_META_CREATED_BY,
      :v_META_LAST_UPDATED_DATE,
      :v_META_LAST_UPDATED_BY,
      :v_NO_DIRTRACKER
    );
END;
/
