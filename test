MERGE INTO sec_contact_temp a
USING (SELECT 1 AS id FROM dual) b
ON (a.LOGIN_ID = b.id)
WHEN MATCHED THEN
  UPDATE SET
    a.ENTERPRISE_ID = 'E001',
    a.EMPLOYEE_ID = 'EMP001',
    a.FIRSTNAME = 'John',
    a.LASTNAME = 'Doe',
    a.PHONE = '1234567890',
    a.EMAIL = 'john.doe@example.com',
    a.TITLE = 'Manager',
    a.ROLE = 'Admin',
    a.MANAGER_ID = 'M001',
    a.MARKET = 'Market1',
    a.SUBMARKET = 'Submarket1',
    a.NO_LDAP_OVERRIDE = 'Y',
    a.ALT_PHONE = '0987654321',
    a.MODIFIED_DATE = TO_DATE('2024-08-20 10:00:00', 'YYYY-MM-DD HH24:MI:SS'),
    a.CREATED_DATE = TO_DATE('2024-08-01 10:00:00', 'YYYY-MM-DD HH24:MI:SS')
WHEN NOT MATCHED THEN
  INSERT (
    LOGIN_ID, ENTERPRISE_ID, EMPLOYEE_ID, FIRSTNAME, LASTNAME, PHONE, EMAIL, TITLE, ROLE, MANAGER_ID, MARKET, SUBMARKET, MODIFIED_DATE, CREATED_DATE, NO_LDAP_OVERRIDE, ALT_PHONE
  )
  VALUES (
    1, 'E001', 'EMP001', 'John', 'Doe', '1234567890', 'john.doe@example.com', 'Manager', 'Admin', 'M001', 'Market1', 'Submarket1', TO_DATE('2024-08-20 10:00:00', 'YYYY-MM-DD HH24:MI:SS'), TO_DATE('2024-08-01 10:00:00', 'YYYY-MM-DD HH24:MI:SS'), 'Y', '0987654321'
  );
